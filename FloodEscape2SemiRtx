a = game.SoundService.Underwater.Equalizer
l = game.Lighting
workspace.Multiplayer:WaitForChild("Map")
map = workspace.Multiplayer.Map:WaitForChild("ExitRegion").Parent
s = Instance.new("BloomEffect",l)


function um()
for i,v in pairs(map:GetDescendants()) do
if v:IsA("BasePart") then
umm = Instance.new("PointLight",v)
umm.Brightness = 0.2
umm.Range = 6
end
end
end


a.HighGain = -50
a.LowGain = 8
a.MidGain = -30


l.Water_Blur.Size = 13
l.Standard_Bloom.Size = 1000
l.Standard_Bloom.Size = 1
s.Size = 1000
s.Name = "Hiix" .. tostring(math.random(1,10000))
s.Intensity = 1
game:GetService("Lighting").ColorCorrection.Saturation = 1
r = game:GetService("Lighting")
r["Water_Bloom"].Intensity = 2


for i,v in pairs(game.Workspace:GetDescendants()) do
if v:IsA("BasePart") then
n = Instance.new("PointLight",v)
n.Brightness = 0.2
n.Range = 6
end
end

map.Removing:Connect(function(lol)
game.Workspace.Multiplayer:WaitForChild("Map")
map = game.Workspace.Multiplayer.Map:WaitForChild("ExitRegion").Parent
um()
end)
