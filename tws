for i,v in pairs(getconnections(game.Players.LocalPlayer.Idled)) do
    v:Disable()
end
x = 15
repeat wait(35)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").MainPlace.DELIVERtm.Boxes.Position)
fireclickdetector(game:GetService("Workspace").MainPlace.DELIVERtm.Boxes.ClickDetector)
wait(.5)
if game.Players.LocalPlayer.Backpack:FindFirstChild("Box") then
game.Players.LocalPlayer.Backpack.Box.Parent = game.Players.LocalPlayer.Character
else
game.Players.LocalPlayer.Backpack["Golden Box"].Parent = game.Players.LocalPlayer.Character
end
wait(20)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").UnderGround.Walls["ELEVATOR 2: THE SEQUEL"].ElevatorShaft.Part.Position)
wait(15)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").UnderGround.Walls.Wood.Position + Vector3.new(0,5,0))
wait(5)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game:GetService("Workspace").MainPlace.DELIVERtm.DeliverBar.Position - Vector3.new(0,1,0))
until x == 16
